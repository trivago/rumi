{
    "name": "trivago/rumi",
    "type": "project",
    "description": "Docker based CI platform that executes jobs definition defined .rumi.yml",
    "keywords": [
        "continuous integration",
        "ci",
        "docker",
        "rumi",
        "trivago"
    ],
    "homepage": "https://github.com/trivago/rumi",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Jakub Sacha",
            "email": "jakub.sacha@trivago.com",
            "homepage": "http://jakubsacha.pl/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/console": "^3.0",
        "symfony/yaml": "^3.0",
        "symfony/process": "^3.0",
        "symfony/dependency-injection": "^3.0",
        "symfony/config": "^3.0",
        "symfony/event-dispatcher": "^3.0",
        "guzzlehttp/guzzle": "^6.2",
        "symfony/serializer": "^3.0",
        "symfony/property-access": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "symfony/var-dumper": "^3.0",
        "mikey179/vfsStream": "^1.6",
        "fabpot/php-cs-fixer": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "Trivago\\Rumi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trivago\\Rumi\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-quick": "phpunit --no-coverage"
    },
    "config": {
        "sort-packages": true
    }
}
